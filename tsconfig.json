{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "module": "CommonJS",
    "rootDir": "src",
    "outDir": "out",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
  }
}